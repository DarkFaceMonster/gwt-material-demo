  <g:HTMLPanel>
  
    <m:MaterialRow addStyleNames="code">
      <m:MaterialTitle title="Introduction" description="You can add a Label as a Tab Item and for contents any Material Widget can be added as a Tab Content (e.g MaterialTopNav)"/>
