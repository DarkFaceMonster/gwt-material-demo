                &emsp;&lt;ma:masonry.MaterialMasonry><br/>
                &emsp;&lt;m:MaterialColumn grid="l4 s12 m6" backgroundColor="BLUE_LIGHTEN_1" height="400px" waves="DEFAULT" padding="0" textAlign="CENTER"><br/>
                &emsp;&lt;m:MaterialIcon iconType="WIFI_TETHERING" iconColor="WHITE" iconSize="LARGE" marginTop="125" /><br/>
